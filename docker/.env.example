# ML Training Server - Docker Compose Environment Variables
# Copy this file to .env and update with your own values

# Grafana
GRAFANA_ADMIN_PASSWORD=changeme_secure_password

# Guacamole Database
GUACAMOLE_DB_PASSWORD=changeme_secure_db_password

# Code-server (VS Code in browser)
CODESERVER_PASSWORD=changeme_secure_codeserver_password

# Jupyter Notebook Token (optional, leave empty for token-based auth)
JUPYTER_TOKEN=

# Domain for Cloudflare Tunnel
DOMAIN=yourdomain.com
